{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/openturk.png",
      "default_popup": "pages/popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery.js", "js/mturk.js"],
      "matches": [ "\u003Call_urls\u003E" ],
      "run_at": "document_end"
   }],
   "description": "Test",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "manifest_version": 2,
   "name": "Openturk",
   "permissions": [ "tabs", "\u003Call_urls\u003E" ],
   "version": "0.0.1"
}
