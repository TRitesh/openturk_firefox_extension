{
   "background": {
     "persistent": false,
     "page": "background.html"
   },
   "browser_action": {
      "default_icon": "icons/icon19.png",
      "default_title": "OpenTurk",
      "default_popup": "pages/popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": ["pages/scripts/jquery-2.0.3.min.js", "js/mturk.js"],
      "matches": [ "http://*.mturk.com/*", "https://*.mturk.com/*"  ],
      "css": [ "pages/styles/button.css" ],
      "run_at": "document_end"
   }],
   "options_page":"pages/options.html",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "manifest_version": 2,
   "name": "Openturk",
   "web_accessible_resources": ["pages/scripts/jquery-2.0.3.min.map"],
   "description": "OpenTurk",
   "permissions": [ "alarms", "tabs", "\u003Call_urls\u003E", "storage" ],
   "version": "0.1.0"
}
